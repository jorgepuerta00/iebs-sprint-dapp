{
  "name": "iebs-sprint-dapp",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/iebs-sprint-dapp/src",
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/iebs-sprint-dapp/**/*.{ts,tsx,js,jsx,vue}"
        ]
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/iebs-sprint-dapp",
        "skipTypeCheck": true,
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/iebs-sprint-dapp/public",
            "output": "./"
          }
        ],
        "styles": [
          "apps/iebs-sprint-dapp/src/style.css"
        ]
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "iebs-sprint-dapp:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "iebs-sprint-dapp:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "iebs-sprint-dapp:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "iebs-sprint-dapp:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "iebs-sprint-dapp:build:development"
        },
        "production": {
          "buildTarget": "iebs-sprint-dapp:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/iebs-sprint-dapp"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "iebs-sprint-dapp:build"
      }
    }
  }
}